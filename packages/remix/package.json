{
	"name": "@myleslinder/eslint-config-remix",
	"version": "0.0.31",
	"description": "ESlint config presets",
	"packageManager": "pnpm@7.11.0",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/myleslinder/eslint-config"
	},
	"homepage": "https://github.com/myleslinder/eslint-config",
	"sideEffects": false,
	"scripts": {
		"release": "pnpm -r publish --scope=@myleslinder --access public"
	},
	"dependencies": {
		"@remix-run/eslint-config": "^1.7.0",
		"@myleslinder/eslint-config-react": "workspace:*",
		"@myleslinder/eslint-config-ts": "workspace:*"
	},
	"devDependencies": {
		"eslint": "^8.5.0"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"react",
				"react-dom"
			]
		}
	},
	"keywords": [],
	"author": "Myles Linder",
	"license": "MIT"
}
